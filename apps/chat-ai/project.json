{
    "name": "chat-ai",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/chat-ai",
    "projectType": "application",
    "tags": [],
    "// targets": "to see all targets run: nx show project chat-ai --web",
    "targets": {
        "dev": {
            "options": {
                "port": 6542,
                "assets": [
                    {
                        "input": "packages/images/src/lib/chat-ai/",
                        "glob": "**.*",
                        "output": "./"
                    }
                ]
            }
        },
        "build": {
            "options": {
                "port": 6542,
                "assets": [
                    {
                        "input": "packages/images/src/lib/chat-ai/",
                        "glob": "**.*",
                        "output": "./"
                    }
                ]
            }
        }
    }
}
